#include "main.h"
/**
 * print_diagonal - This function draws a diagonal line on the terminal
 *
 * @size: is the size of the square
 *
 */

void print_square(int size)
{
	int i;
	int j;

	if (size > 0)
	{
		for (i = 0; i < n; i++)
		{
			for (j = 0; j < i; j++)
			{
				_putchar(32);
			}
			_putchar(92);
			_putchar('\n');
		}
	} else
	{
	_putchar('\n');
	}
}
