#include <ctype.h>
#include <stdio.h>
/**
 * _islower - function that checks for lowercase character
 * @c: The character to check
 *
 * Return: 1 if charater is lowercase, 0 otherwise
 */

int _islower(int c)
{
	printf("%d\n", c);
	// printf("%d", islower(c));
	return (islower(c));
}
int main(void)
{
	int r;
	int r2;

	r = _islower('H');
	printf("%d\n", r);
	r = _islower('o');
	printf("%d\n", r2);
}
