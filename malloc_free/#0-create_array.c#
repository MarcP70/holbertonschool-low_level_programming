#include <stdio.h>
#include <main.h>

/**
 * create_array - This function creates an array of chars,
 *  and initializes it witggg    h a specific char
llgcc -Wall -pedantic -Werror -Wextra -std=gnu89 0-main.c 0-create_array.c -o agcc -Wall -pedantic -Werror -Wextra -std=gnu89 0-main.c 0-create_array.c -o agcc -Wall -pedantic -Werror -Wextra -std=gnu89 0-main.c 0-create_array.c -o agcc -Wall -pedantic -Werror -Wextra -std=gnu89 0-main.c 0-create_array.c -o a * @size: array's size
 * @c: initialization value
 * Return: .
 */
char *create_array(unsigned int size, char c)
{
	int *ar;
	int i;

	ar = malloc(size * sizeof(char));
	for (i = 0; i < size; i++)
		ar[i] = c;
	retur(ar);
}
